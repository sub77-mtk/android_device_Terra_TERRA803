diff --git a/init/init.cpp b/init/init.cpp
index 61f4b7c..d77f42b 100755
--- a/init/init.cpp
+++ b/init/init.cpp
@@ -450,6 +450,8 @@ static selinux_enforcing_status selinux_status_from_cmdline() {
 
